{
   "name": "blitzinfo",
   "version": "0.0.1",
   "scripts": {
      "lint": "eslint \"**/*.ts\" --fix",
      "rimraf": "rimraf dist",
      "copy-assets": "copyfiles \"static-json-data/*.json\" package*.json \"init-on*\" init.json dist",
      "build-webpack": "webpack --config webpack.main.app.config.js && webpack --config webpack.revgeo.app.config.js && webpack --config webpack.test-output.config.js",
      "prestart-local": "npm run build",
      "start-local": "pushd dist && init-on-windows.cmd",
      "build": "npm run rimraf && npm run build-webpack && npm run copy-assets",
      "deploy": "npm run build && node ssh-deployment",
      "generate-barrels": "barrelsby -D -e spec.ts$ -d ./app/database && barrelsby -D -e spec.ts$ -d ./app/helpers && barrelsby -D -e spec.ts$ -d ./app/rest && barrelsby -D -e spec.ts$ -d ./app/reverse-geocoding && barrelsby -D -e spec.ts$ -d ./app/services",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json"
   },
   "dependencies": {
      "body-parser": "^1.19.0",
      "cli-color": "^1.4.0",
      "commander": "^2.20.3",
      "cors": "^2.8.5",
      "express": "^4.17.1",
      "geojson-utils": "^1.1.0",
      "getjson": "0.0.1",
      "gm": "^1.23.1",
      "htmlparser2": "^3.10.1",
      "kareem": "^2.3.2",
      "lodash": "^4.17.20",
      "method-override": "^3.0.0",
      "moment": "^2.29.1",
      "mongoose": "^5.11.14",
      "mongoose-auto-increment-reworked": "^1.2.1",
      "morgan": "^1.10.0",
      "request": "^2.88.2",
      "requestify": "^0.2.5",
      "serve-static": "^1.14.1",
      "socket.io": "^2.4.1",
      "swagger-express": "^1.0.5",
      "websocket": "^1.0.33",
      "wherewolf": "^1.0.3"
   },
   "devDependencies": {
      "@morgan-stanley/ts-mocking-bird": "^0.4.0",
      "@types/body-parser": "^1.19.0",
      "@types/cli-color": "^0.3.30",
      "@types/concaveman": "^1.1.3",
      "@types/cors": "^2.8.9",
      "@types/express": "^4.17.11",
      "@types/geojson": "^7946.0.7",
      "@types/gm": "^1.18.9",
      "@types/googlemaps": "^3.43.2",
      "@types/htmlparser2": "^3.10.2",
      "@types/jest": "^26.0.20",
      "@types/lodash": "^4.14.168",
      "@types/method-override": "0.0.31",
      "@types/mime": "^2.0.3",
      "@types/moment": "^2.13.0",
      "@types/mongoose": "^5.10.3",
      "@types/morgan": "^1.9.2",
      "@types/node": "^14.14.22",
      "@types/serve-static": "^1.13.9",
      "@types/socket.io": "^2.1.13",
      "@types/socket.io-client": "^1.4.35",
      "@types/websocket": "0.0.39",
      "@typescript-eslint/eslint-plugin": "^4.14.1",
      "@typescript-eslint/parser": "^4.14.1",
      "barrelsby": "^2.2.0",
      "copyfiles": "^2.4.1",
      "eslint": "^7.18.0",
      "eslint-config-prettier": "^7.2.0",
      "eslint-plugin-prettier": "^3.3.1",
      "jest": "^26.6.3",
      "node-ssh": "^5.1.2",
      "prettier": "^2.2.1",
      "rimraf": "^2.7.1",
      "rxjs": "^6.6.3",
      "ts-jest": "^26.5.0",
      "ts-loader": "^8.0.14",
      "ts-node": "^9.1.1",
      "tslint": "^6.1.3",
      "tslint-config-prettier": "^1.18.0",
      "tslint-plugin-prettier": "^2.3.0",
      "typescript": "^4.1.3",
      "webpack": "^5.18.0",
      "webpack-cli": "^4.4.0",
      "webpack-node-externals": "^1.7.2"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "test",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
