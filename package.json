{
  "name": "blitzinfo",
  "version": "0.0.1",
  "scripts": {
    "lint": "tslint -p tslint.json",
    "rimraf": "rimraf dist",
    "copy-assets": "copyfiles \"static-json-data/*.json\" package*.json \"init-on*\" init.json dist",
    "build-webpack": "webpack --config webpack.main.app.config.js && webpack --config webpack.revgeo.app.config.js && webpack --config webpack.test-output.config.js",
    "build": "npm run rimraf && npm run build-webpack && npm run copy-assets",
    "deploy": "npm run build && node ssh-deployment",
    "generate-barrels": "barrelsby -D -e spec.ts$ -d ./app/database && barrelsby -D -e spec.ts$ -d ./app/helpers && barrelsby -D -e spec.ts$ -d ./app/rest && barrelsby -D -e spec.ts$ -d ./app/reverse-geocoding && barrelsby -D -e spec.ts$ -d ./app/services"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cli-color": "^1.1.0",
    "commander": "^2.16.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "geojson-utils": "^1.1.0",
    "getjson": "0.0.1",
    "gm": "^1.23.1",
    "htmlparser2": "^3.9.2",
    "kareem": "^2.2.1",
    "lodash": "^4.17.10",
    "method-override": "^3.0.0",
    "moment": "^2.22.2",
    "mongodb": "^3.1.1",
    "mongoose": "^5.2.6",
    "mongoose-auto-increment-reworked": "^1.2.1",
    "morgan": "^1.9.0",
    "request": "^2.87.0",
    "requestify": "^0.2.5",
    "rxjs": "^6.2.2",
    "serve-static": "^1.13.2",
    "socket.io": "^2.1.1",
    "swagger-express": "^1.0.5",
    "websocket": "^1.0.26",
    "wherewolf": "^1.0.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cli-color": "^0.3.29",
    "@types/concaveman": "^1.1.3",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/geojson": "^7946.0.4",
    "@types/gm": "^1.18.0",
    "@types/googlemaps": "^3.30.11",
    "@types/htmlparser2": "^3.7.31",
    "@types/lodash": "^4.14.116",
    "@types/method-override": "0.0.31",
    "@types/mime": "^2.0.0",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.2.2",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.5.5",
    "@types/serve-static": "^1.13.2",
    "@types/socket.io": "^1.4.36",
    "@types/socket.io-client": "^1.4.32",
    "@types/websocket": "0.0.39",
    "barrelsby": "^1.0.2",
    "copyfiles": "^2.0.0",
    "grunt-ts": "^6.0.0-beta.21",
    "node-ssh": "^5.1.2",
    "rimraf": "^2.6.2",
    "rxjs-tslint": "^0.1.5",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  }
}
