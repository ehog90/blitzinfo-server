{
   "name": "blitzinfo",
   "version": "0.0.1",
   "scripts": {
      "fix-lint": "npx tslint -c tslint.conf.js -p ./ --fix",
      "lint": "npx tslint -c tslint.conf.js -p ./",
      "rimraf": "rimraf dist",
      "copy-assets": "copyfiles \"static-json-data/*.json\" package*.json \"init-on*\" init.json dist",
      "build-webpack": "webpack --config webpack.main.app.config.js && webpack --config webpack.revgeo.app.config.js && webpack --config webpack.test-output.config.js",
      "build": "npm run rimraf && npm run build-webpack && npm run copy-assets",
      "deploy": "npm run build && node ssh-deployment",
      "generate-barrels": "barrelsby -D -e spec.ts$ -d ./app/database && barrelsby -D -e spec.ts$ -d ./app/helpers && barrelsby -D -e spec.ts$ -d ./app/rest && barrelsby -D -e spec.ts$ -d ./app/reverse-geocoding && barrelsby -D -e spec.ts$ -d ./app/services"
   },
   "dependencies": {
      "body-parser": "^1.19.0",
      "cli-color": "^1.4.0",
      "commander": "^2.20.3",
      "cors": "^2.8.5",
      "express": "^4.17.1",
      "geojson-utils": "^1.1.0",
      "getjson": "0.0.1",
      "gm": "^1.23.1",
      "htmlparser2": "^3.10.1",
      "kareem": "^2.3.1",
      "lodash": "^4.17.15",
      "method-override": "^3.0.0",
      "moment": "^2.26.0",
      "mongoose": "^5.9.16",
      "mongoose-auto-increment-reworked": "^1.2.1",
      "morgan": "^1.10.0",
      "request": "^2.88.2",
      "requestify": "^0.2.5",
      "serve-static": "^1.14.1",
      "socket.io": "^2.4.0",
      "swagger-express": "^1.0.5",
      "websocket": "^1.0.31",
      "wherewolf": "^1.0.3"
   },
   "devDependencies": {
      "@types/body-parser": "^1.19.0",
      "@types/cli-color": "^0.3.30",
      "@types/concaveman": "^1.1.3",
      "@types/cors": "^2.8.6",
      "@types/express": "^4.17.6",
      "@types/geojson": "^7946.0.7",
      "@types/gm": "^1.18.8",
      "@types/googlemaps": "^3.39.6",
      "@types/htmlparser2": "^3.10.1",
      "@types/lodash": "^4.14.152",
      "@types/method-override": "0.0.31",
      "@types/mime": "^2.0.2",
      "@types/moment": "^2.13.0",
      "@types/mongoose": "^5.7.21",
      "@types/morgan": "^1.9.0",
      "@types/node": "^14.0.5",
      "@types/serve-static": "^1.13.4",
      "@types/socket.io": "^2.1.8",
      "@types/socket.io-client": "^1.4.33",
      "@types/websocket": "0.0.39",
      "barrelsby": "^1.0.2",
      "copyfiles": "^2.2.0",
      "grunt-ts": "^6.0.0-beta.22",
      "node-ssh": "^5.1.2",
      "prettier": "^2.0.5",
      "rimraf": "^2.7.1",
      "rxjs": "^6.5.5",
      "rxjs-tslint": "^0.1.8",
      "ts-loader": "^4.5.0",
      "tslint": "^6.1.2",
      "tslint-config-prettier": "^1.18.0",
      "tslint-plugin-prettier": "^2.3.0",
      "typescript": "^3.9.3",
      "webpack": "^4.43.0",
      "webpack-cli": "^3.3.11",
      "webpack-node-externals": "^1.7.2"
   }
}
